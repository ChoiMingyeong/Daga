@using DagaCommon.Models
<MudNavMenu>
    <MudLink Typo="Typo.h6" Class="px-4" Href="">
        <MudIcon Icon="@Icons.Material.Filled.EnergySavingsLeaf" />
        DagaTools
    </MudLink>
    <MudSelect @bind-Value="AuthService.SelectedProject"
               ReadOnly="true"
               Variant="Variant.Outlined"
               Label="Project"
               Dense="true"
               Placeholder="---Select Project---">
        @foreach (var project in AuthService.Projects)
        {
            <MudSelectItem Value="@project">@project.Name</MudSelectItem>
        }
    </MudSelect>
    <MudDivider Class="my-2" />
    <MudNavLink Href="/datatable">데이터테이블</MudNavLink>
</MudNavMenu>

@code {
    protected override Task OnInitializedAsync()
    {

        return base.OnInitializedAsync();
    }
}